{
  "name": "pennsieve-viz-monorepo",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @pennsieve-viz/lib dev",
    "build": "pnpm --filter @pennsieve-viz/lib build && pnpm --filter @pennsieve-viz/tsviewer build",
    "build:lib": "pnpm --filter @pennsieve-viz/lib build",
    "build:tsviewer": "pnpm --filter @pennsieve-viz/tsviewer build",
    "type-check": "pnpm -r type-check",
    "clean": "pnpm -r exec rm -rf dist",
    "lint": "eslint packages --ext .ts,.vue",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish"
  },
  "devDependencies": {
    "@aws-amplify/auth": "^6.13.0",
    "@changesets/cli": "^2.27.10",
    "@types/d3": "^7.4.3",
    "@types/dompurify": "^3.0.5",
    "@types/ramda": "^0.30.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/tsconfig": "^0.7.0",
    "sass-embedded": "^1.90.0",
    "typescript": "^5.8.3",
    "vite": "^6.0.0-alpha.24",
    "vite-plugin-dts": "^4.3.0",
    "vue": "^3.5.17",
    "vue-tsc": "^3.0.3",
    "pinia": "^3.0.3"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@9.14.2"
}
