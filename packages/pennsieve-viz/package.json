  },
  "peerDependenciesMeta": {
    "plotly.js": {
      "optional": true
    },
    "@aws-amplify/auth": {
      "optional": true
    },
    "@pennsieve-viz/tsviewer": {
      "optional": true
    },
    "@pennsieve-viz/micro-ct": {
      "optional": true
    }
  },
  "repository": {
